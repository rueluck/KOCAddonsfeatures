uW.btMonitorExternalCall = function(name) {
	if (name !="") {
		document.getElementById('btPlayer').value = name;	 
		MonitorTRClick();
	}	
}	

uW.btMonitorExternalCallUID = function(UID) {
	if (UID !="") {
		initMonitor (UID, false)
	}	
}	

// Functions

function btStartup (){
	clearTimeout (btStartupTimer);
	if (uW.btLoaded)
		return;
