// @exclude        			for (var i = 0; i < findReplace.length; i++) {
// @exclude        				this.modsActive[i] = false;
// @exclude        			}
// @exclude        		}
// @exclude        	} catch (err) {
// @exclude        		logit("CalterFuncModifier " + this.funcName + " " + err);
// @exclude        	}
// @exclude        	// test if this modifier works on the original function.
// @exclude        	//    true = match found / replace possible
// @exclude        	//    false = does not match
// @exclude        	function testModifier(modNumber) {
// @exclude        		x = this.funcOldString.replace(this.modifiers[modNumber][0], this.modifiers[modNumber][1]);
// @exclude        		if (x != this.funcOldString)
// @exclude        		{
// @exclude        			return true;
// @exclude        		}
// @exclude        		return false;
// @exclude        	}
// @exclude        	
// @exclude        	// use the active modifiers to create/apply a new function
// @exclude        	function applyModifiers() {
// @exclude        		try {
// @exclude        			var rt = this.funcOldString;
// @exclude        			var active = false;
// @exclude        			for (var i = 0; i < this.modifiers.length; i++) {
// @exclude        				if (!this.modsActive[i]) continue;
// @exclude        				x = rt.replace(this.modifiers[i][0], this.modifiers[i][1]);
// @exclude        				if (x == rt) // if not found
// @exclude        				{
// @exclude        					// print out an error message when the match fails.
// @exclude        					// These messages get lost on a refresh, so wait a few seconds to put it in the error log.
// @exclude        					setTimeout(function (fname, repStr, ftstr) {
// @exclude        						return function () {
// @exclude        							logit("Unable to replace string in function " + fname);
// @exclude        							logit("Replacment string:" + repStr);
// @exclude        							logit("Function listing: " + ftstr);
// @exclude        							return;
// @exclude        						}
// @exclude        					}(this.funcName, this.modifiers[i][0], ft), 3000);
// @exclude        				} 
// @exclude        				else {
// @exclude        				
// @exclude        				}
// @exclude        				rt = x;
// @exclude        				active = true;
// @exclude        			}
// @exclude        			
// @exclude        			this.funcNew = rt;
// @exclude        			if (active) {
// @exclude        				// apply the new function
// @exclude        				uW.uwuwuwFunc(this.funcName + ' = ' + this.funcNew);
// @exclude        			} else {
// @exclude        				// set to the original function
// @exclude        				var x1 = this.funcName.split('.');
// @exclude        				var f1 = unsafeWindow;
// @exclude        				for (var i = 0; i < x1.length - 1; i++)
// @exclude        					f1 = f1[x1[i]];
// @exclude        				f1[x1[x1.length - 1]] = this.funcOld;
// @exclude        			}
// @exclude        		} catch (err) {
// @exclude        			logit("CalterFuncModifier " + this.funcName + " " + err);
// @exclude        		}
// @exclude        	}
// @exclude        	
// @exclude        	// add additional modifiers.  The index of the modifier is returned so the caller can enable/disable it specificially
// @exclude        	function addModifier(fr) {
// @exclude        		this.modifiers.push(fr);
// @exclude        		this.modsActive.push(false);
// @exclude        		// return the index of the newly added modifier
// @exclude        		return this.modifiers.length - 1;
// @exclude        	}
// @exclude        	
// @exclude        	// turn on/off some of the modifiers.
// @exclude        	// 'len' allows setting consectutive modifiers to the same value.
// @exclude        	//   If len is null, 1 is used
// @exclude        	function enableModifier(modNumber, value, len) {
// @exclude        	
// @exclude        		if (len == null) len = 1;
// @exclude        		for (i = modNumber; i < modNumber + len; i++) {
// @exclude        			if (i < this.modsActive.length) {
// @exclude        				this.modsActive[i] = value;
// @exclude        			}
// @exclude        		}
// @exclude        		this.applyModifiers();
// @exclude        	}
