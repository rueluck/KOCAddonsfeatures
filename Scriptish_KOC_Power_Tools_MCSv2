// @exclude        				if (newTop < t.bounds.top) { // if out-of-bounds...
// @exclude        					newTop = t.bounds.top;
// @exclude        					_doneMoving(t);
// @exclude        				} else if (newLeft < t.bounds.left) {
// @exclude        					newLeft = t.bounds.left;
// @exclude        					_doneMoving(t);
// @exclude        				} else if (newLeft > t.bounds.right) {
// @exclude        					newLeft = t.bounds.right;
// @exclude        					_doneMoving(t);
// @exclude        				} else if (newTop > t.bounds.bot) {
// @exclude        					newTop = t.bounds.bot;
// @exclude        					_doneMoving(t);
// @exclude        				}
// @exclude        				t.theDiv.style.top = newTop + 'px';
// @exclude        				t.theDiv.style.left = newLeft + 'px';
// @exclude        				t.lastX = me.clientX;
// @exclude        				t.lastY = me.clientY;
// @exclude        			}
// @exclude        		}
// @exclude        	}
// @exclude        	function debug(msg, e) {
