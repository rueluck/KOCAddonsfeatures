// @exclude        		}, false);
// @exclude        		t.modifyUnitResearch();
// @exclude        	},
// @exclude        	modifyUnitResearch: function () {
// @exclude        		var t = Tabs.UnitCalc;
// @exclude        		var resLife = (5 * parseInt(document.getElementById('ptucResHP').value) / 100);
// @exclude        		var resAtk = (5 * parseInt(document.getElementById('ptucResPE').value) / 100);
// @exclude        		var resDef = (5 * parseInt(document.getElementById('ptucResMA').value) / 100);
// @exclude        		var resSpd = (5 * parseInt(document.getElementById('ptucResAH').value) / 100);
// @exclude        		var resRng = (5 * parseInt(document.getElementById('ptucResFL').value) / 100);
// @exclude        		resRng += (2 * parseInt(document.getElementById('ptucResIF').value) / 100);
// @exclude        		var knight = parseFloat(document.getElementById('ptucKnightLevel').value) / 200;
// @exclude        		var guardLife = t.woodGuardTable(parseInt(document.getElementById('ptucWood').value));
// @exclude        		var guardAtk = t.oreGuardTable(parseInt(document.getElementById('ptucOre').value));
// @exclude        		var guardLifeAct = document.getElementById('ptucWoodAct').checked ? 1 : 0;
// @exclude        		var guardAtkAct = document.getElementById('ptucOreAct').checked ? 1 : 0;
// @exclude        		var guardSetAct = document.getElementById('ptucGuardSet').checked ? 1 : 0;
// @exclude        		var guardOreBless = document.getElementById('ptucOreBless').checked ? 1 : 0;
// @exclude        		var bloodLustBlessLife = document.getElementById('ptucBloodBless').checked ? 0.75 : 1;
// @exclude        		var bloodLustBlessAtkSpd = document.getElementById('ptucBloodBless').checked ? 1.5 : 1;
// @exclude        		var defending = document.getElementById('ptucDefending').checked ? 1 : 0;
// @exclude        		var itemAtk = 0;
// @exclude        		var itemDef = 0;
// @exclude        		var feyAltarAct = document.getElementById('ptucFeyAltarActive').checked ? 1 : 0;
// @exclude        		var feyAltar = parseFloat(document.getElementById('ptucFeyAltarBonus').value) / 100;
// @exclude        		var orderDef = 0;
// @exclude        		var champLife = parseFloat(document.getElementById('ptucLifeChampMod').value);
// @exclude        		var champAtk = parseFloat(document.getElementById('ptucAtkChampMod').value);
// @exclude        		var champDef = parseFloat(document.getElementById('ptucDefChampMod').value);
// @exclude        		var champSpd = parseFloat(document.getElementById('ptucSpdChampMod').value);
// @exclude        		var champRng = parseFloat(document.getElementById('ptucRngChampMod').value);
// @exclude        		if (document.getElementById('ptucOrder').checked)
// @exclude        			orderDef = 0.15;
// @exclude        		if (document.getElementById('ptucItemAtk20').checked)
// @exclude        			itemAtk = 0.2 + itemAtk;
// @exclude        		if (document.getElementById('ptucItemAtk50').checked)
// @exclude        			itemAtk = 0.5 + itemAtk;
// @exclude        		if (document.getElementById('ptucItemDef20').checked)
// @exclude        			itemDef = 0.2 + itemDef;
// @exclude        		if (document.getElementById('ptucItemDef50').checked)
// @exclude        			itemDef = 0.5 + itemDef;
// @exclude        		if (defending)
// @exclude        			bloodLustBlessAtkSpd = 1;
// @exclude        		// calculate guardian
// @exclude        		if (guardSetAct) { //if you have set bonus
// @exclude        			if (guardLifeAct && defending) { //if your want defending troop stats
// @exclude        				guardLife = (guardLife / 2 + guardLife) / 100;
// @exclude        				guardAtk = 0;
// @exclude        			} else if (guardAtkAct) {
// @exclude        				if (defending) {
// @exclude        					guardAtk = 0;
// @exclude        					guardLife = guardLife / 200;
// @exclude        				} else {
// @exclude        					guardAtk = (1.5 * guardAtk / 100) + guardOreBless * 0.15 + guardOreBless * (1.5 * guardAtk / 100);
// @exclude        					guardLife = 0;
// @exclude        				}
// @exclude        			} else {
// @exclude        				if (defending) {
// @exclude        					guardAtk = 0;
// @exclude        					guardLife = guardLife / 200;
// @exclude        				} else {
// @exclude        					guardAtk = (guardAtk / 200) + guardOreBless * 0.15 + guardOreBless * 0.15 * (guardAtk / 200);
// @exclude        					guardLife = 0;
// @exclude        				}
// @exclude        			}
// @exclude        		} else { // don't have set bonus
// @exclude        			if (guardLifeAct && defending) {
// @exclude        				guardLife = guardLife / 100;
// @exclude        				guardAtk = 0;
// @exclude        			} else if (guardAtkAct && !defending) {
// @exclude        				guardAtk = (guardAtk / 100) + guardOreBless * 0.15 + guardOreBless * 0.15 * (guardAtk / 100);
// @exclude        				guardLife = 0;
// @exclude        			} else {
// @exclude        				guardAtk = 0;
// @exclude        				guardLife = 0;
// @exclude        			}
// @exclude        		}
// @exclude        		var ui;
// @exclude        		for (var iu in uW.cm.UNIT_TYPES) {
// @exclude        			ui = uW.cm.UNIT_TYPES[iu];
// @exclude        			var lifchampfeyadj = champLife + (1 + feyAltar * feyAltarAct) * uW.unitstats['unt' + ui][0];
// @exclude        			lifchampfeyadj = Math.max(lifchampfeyadj, (1 + feyAltar * feyAltarAct) * uW.unitstats['unt' + ui][0] * 0.01);
// @exclude        			var atkchampfeyadj = champAtk + (1 + feyAltar * feyAltarAct) * uW.unitstats['unt' + ui][1];
// @exclude        			atkchampfeyadj = Math.max(atkchampfeyadj, (1 + feyAltar * feyAltarAct) * uW.unitstats['unt' + ui][1] * 0.01);
// @exclude        			var defchampfeyadj = champDef + (1 + feyAltar * feyAltarAct) * uW.unitstats['unt' + ui][2];
// @exclude        			defchampfeyadj = Math.max(defchampfeyadj, (1 + feyAltar * feyAltarAct) * uW.unitstats['unt' + ui][2] * 0.01);
// @exclude        			var spdchampfeyadj = champSpd + (1 + feyAltar * feyAltarAct) * uW.unitstats['unt' + ui][3];
// @exclude        			spdchampfeyadj = Math.max(spdchampfeyadj, (1 + feyAltar * feyAltarAct) * uW.unitstats['unt' + ui][3] * 0.01);
// @exclude        			var rngchampfeyadj = champRng + (1 + feyAltar * feyAltarAct) * uW.unitstats['unt' + ui][4];
// @exclude        			rngchampfeyadj = Math.max(rngchampfeyadj, (1 + feyAltar * feyAltarAct) * uW.unitstats['unt' + ui][4] * 0.01);
// @exclude        			switch (unsafeWindow.cm.unitFrontendType[ui]) {
// @exclude        			case "infantry":
// @exclude        				if (ui < 10) {
// @exclude        					document.getElementById('ptucTrp' + ui + 'Life').innerHTML = t.round1decimals((1 + guardLife) * lifchampfeyadj * bloodLustBlessLife * (1 + (resLife + t.maxBuff('Life', parseFloat(document.getElementById('ptucLifeMod').value), parseFloat(document.getElementById('ptucLifeModInf').value)) / 100)));
// @exclude        					document.getElementById('ptucTrp' + ui + 'Atk').innerHTML = t.round1decimals((1 + guardAtk) * atkchampfeyadj * bloodLustBlessAtkSpd * (1 + (resAtk + knight + itemAtk + t.maxBuff('Attack', parseFloat(document.getElementById('ptucAtkMod').value), parseFloat(document.getElementById('ptucAtkModInf').value)) / 100)));
// @exclude        					document.getElementById('ptucTrp' + ui + 'Def').innerHTML = t.round1decimals(defchampfeyadj * (1 + (resDef + knight + itemDef + orderDef + t.maxBuff('Defense', parseFloat(document.getElementById('ptucDefMod').value), parseFloat(document.getElementById('ptucDefModInf').value)) / 100)));
// @exclude        					document.getElementById('ptucTrp' + ui + 'Spd').innerHTML = t.round1decimals(spdchampfeyadj * (1 + (t.maxBuff('Speed', parseFloat(document.getElementById('ptucSpdMod').value), parseFloat(document.getElementById('ptucSpdModInf').value)) / 100)));
// @exclude        					document.getElementById('ptucTrp' + ui + 'Rng').innerHTML = t.round1decimals(rngchampfeyadj * (1 + (t.maxBuff('Range', parseFloat(document.getElementById('ptucRngMod').value), parseFloat(document.getElementById('ptucRngModInf').value)) / 100)));
// @exclude        				} else {
// @exclude        					//Trp13 - blood
// @exclude        					//verified on 11/30 that bloods don't use infantry buff for atk/def. other stats unknown
// @exclude        					//Trp14 - exec
// @exclude        					//verified on 11/30 that exec don't use infantry buff for atk/def. other stats unknown
// @exclude        					document.getElementById('ptucTrp' + ui + 'Life').innerHTML = t.round1decimals((1 + guardLife) * lifchampfeyadj * bloodLustBlessLife * (1 + (resLife + t.maxBuff('Life', parseFloat(document.getElementById('ptucLifeMod').value), 0) / 100)));
// @exclude        					document.getElementById('ptucTrp' + ui + 'Atk').innerHTML = t.round1decimals((1 + guardAtk) * atkchampfeyadj * bloodLustBlessAtkSpd * (1 + (resAtk + knight + itemAtk + t.maxBuff('Attack', parseFloat(document.getElementById('ptucAtkMod').value), 0) / 100)));
// @exclude        					document.getElementById('ptucTrp' + ui + 'Def').innerHTML = t.round1decimals(defchampfeyadj * (1 + (resDef + knight + itemDef + orderDef + t.maxBuff('Defense', parseFloat(document.getElementById('ptucDefMod').value), 0) / 100)));
// @exclude        					document.getElementById('ptucTrp' + ui + 'Spd').innerHTML = t.round1decimals(spdchampfeyadj * (1 + (t.maxBuff('Speed', parseFloat(document.getElementById('ptucSpdMod').value), 0) / 100)));
// @exclude        					document.getElementById('ptucTrp' + ui + 'Rng').innerHTML = t.round1decimals(rngchampfeyadj * (1 + (t.maxBuff('Range', parseFloat(document.getElementById('ptucRngMod').value), 0) / 100)));
// @exclude        				}
// @exclude        				break;
// @exclude        			case "ranged":
// @exclude        				document.getElementById('ptucTrp' + ui + 'Life').innerHTML = t.round1decimals((1 + guardLife) * lifchampfeyadj * bloodLustBlessLife * (1 + (resLife + t.maxBuff('Life', parseFloat(document.getElementById('ptucLifeMod').value), parseFloat(document.getElementById('ptucLifeModRng').value)) / 100)));
// @exclude        				document.getElementById('ptucTrp' + ui + 'Atk').innerHTML = t.round1decimals((1 + guardAtk) * atkchampfeyadj * bloodLustBlessAtkSpd * (1 + (resAtk + knight + itemAtk + t.maxBuff('Attack', parseFloat(document.getElementById('ptucAtkMod').value), parseFloat(document.getElementById('ptucAtkModRng').value)) / 100)));
// @exclude        				document.getElementById('ptucTrp' + ui + 'Def').innerHTML = t.round1decimals(defchampfeyadj * (1 + (resDef + knight + itemDef + orderDef + t.maxBuff('Defense', parseFloat(document.getElementById('ptucDefMod').value), parseFloat(document.getElementById('ptucDefModRng').value)) / 100)));
// @exclude        				document.getElementById('ptucTrp' + ui + 'Spd').innerHTML = t.round1decimals(spdchampfeyadj * (1 + (t.maxBuff('Speed', parseFloat(document.getElementById('ptucSpdMod').value), parseFloat(document.getElementById('ptucSpdModRng').value)) / 100)));
// @exclude        				document.getElementById('ptucTrp' + ui + 'Rng').innerHTML = t.round1decimals(rngchampfeyadj * (1 + (resRng + t.maxBuff('Range', parseFloat(document.getElementById('ptucRngMod').value), parseFloat(document.getElementById('ptucRngModRng').value)) / 100)));
// @exclude        				break;
// @exclude        			case "horsed":
// @exclude        				document.getElementById('ptucTrp' + ui + 'Life').innerHTML = t.round1decimals((1 + guardLife) * lifchampfeyadj * (1 + (resLife + t.maxBuff('Life', parseFloat(document.getElementById('ptucLifeMod').value), parseFloat(document.getElementById('ptucLifeModHor').value)) / 100)));
// @exclude        				document.getElementById('ptucTrp' + ui + 'Atk').innerHTML = t.round1decimals((1 + guardAtk) * atkchampfeyadj * (1 + (resAtk + knight + itemAtk + t.maxBuff('Attack', parseFloat(document.getElementById('ptucAtkMod').value), parseFloat(document.getElementById('ptucAtkModHor').value)) / 100)));
// @exclude        				document.getElementById('ptucTrp' + ui + 'Def').innerHTML = t.round1decimals(defchampfeyadj * (1 + (resDef + knight + itemDef + orderDef + t.maxBuff('Defense', parseFloat(document.getElementById('ptucDefMod').value), parseFloat(document.getElementById('ptucDefModHor').value)) / 100)));
// @exclude        				document.getElementById('ptucTrp' + ui + 'Spd').innerHTML = t.round1decimals(spdchampfeyadj * (1 + (resSpd + t.maxBuff('Speed', parseFloat(document.getElementById('ptucSpdMod').value), parseFloat(document.getElementById('ptucSpdModHor').value)) / 100)));
// @exclude        				document.getElementById('ptucTrp' + ui + 'Rng').innerHTML = t.round1decimals(rngchampfeyadj * (1 + (t.maxBuff('Range', parseFloat(document.getElementById('ptucRngMod').value), parseFloat(document.getElementById('ptucRngModHor').value)) / 100)));
