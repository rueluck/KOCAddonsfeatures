// @exclude        	try {
// @exclude        		var x = this.funcName.split('.');
// @exclude        		var f = unsafeWindow;
// @exclude        		for (var i = 0; i < x.length; i++)
// @exclude        			f = f[x[i]];
// @exclude        		ft = f.toString();
