// @exclude        	}
// @exclude        	function dispEvent(msg, me) {
// @exclude        		logit(msg + ' Button:' + me.button + ' Screen:' + me.screenX + ',' + me.screenY + ' client:' + me.clientX + ',' + me.clientY + ' rTarget: ' + me.relatedTarget);
// @exclude        	}
// @exclude        }
// @exclude        function inspect(obj, maxLevels, level, doFunctions) {
// @exclude        	var str = '',
// @exclude        		type, msg;
// @exclude        	if (level == null) level = 0;
// @exclude        	if (maxLevels == null) maxLevels = 1;
// @exclude        	if (maxLevels < 1)
// @exclude        		return 'Inspect Error: Levels number must be > 0';
// @exclude        	if (obj == null)
// @exclude        		return 'ERROR: Object is NULL\n';
// @exclude        	var indent = '';
// @exclude        	for (var i = 0; i < level; i++)
// @exclude        		indent += '  ';
// @exclude        	for (property in obj) {
// @exclude        		try {
// @exclude        			type = matTypeof(obj[property]);
// @exclude        			if (doFunctions == true && (type == 'function')) {
// @exclude        				str += indent + '(' + type + ') ' + property + "[FUNCTION]\n";
// @exclude        			} else if (type != 'function') {
// @exclude        				str += indent + '(' + type + ') ' + property + ((obj[property] == null) ? (': null') : ('')) + ' = ' + obj[property] + "\n";
// @exclude        			}
// @exclude        			if ((type == 'object' || type == 'array') && (obj[property] != null) && (level + 1 < maxLevels))
// @exclude        				str += inspect(obj[property], maxLevels, level + 1, doFunctions); // recurse
// @exclude        		} catch (err) {
// @exclude        			// Is there some properties in obj we can't access? Print it red.
// @exclude        			if (typeof (err) == 'string') msg = err;
