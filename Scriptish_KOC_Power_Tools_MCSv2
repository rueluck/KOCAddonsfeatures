// @exclude        var CalterFuncModifier = function (funcName, findReplace) {
// @exclude        	// (second argument is now optional )
// @exclude        	this.applyModifiers = applyModifiers;
// @exclude        	this.addModifier = addModifier;
// @exclude        	this.enableModifier = enableModifier;
