// @exclude        		else
// @exclude        			wilds[type / 10] += parseInt(w[k].tileLevel);
// @exclude        	}
// @exclude        	knight = 0;
// @exclude        	var s = Seed.knights["city" + cityId];
// @exclude        	if (s) {
// @exclude        		s = s["knt" + Seed.leaders["city" + cityId].resourcefulnessKnightId];
