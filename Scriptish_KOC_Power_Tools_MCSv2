// @exclude        function getDST(today) {
// @exclude        	var yr = today.getFullYear();
// @exclude        	var dst_start = new Date(yr+"-03-14T02:00:00"); // 2nd Sunday in March can't occur after the 14th 
// @exclude        	var dst_end = new Date(yr+"-11-07T02:00:00"); // 1st Sunday in November can't occur after the 7th
