// @exclude        				knight *= 1.25;
// @exclude        		}
// @exclude        	}
// @exclude        	var workerFactor = 1;
// @exclude        	var c = parseInt(Seed.citystats["city" + cityId]["pop"][0]); // Current  population
// @exclude        	var w = parseInt(Seed.citystats["city" + cityId]["pop"][3]); // Labor force
