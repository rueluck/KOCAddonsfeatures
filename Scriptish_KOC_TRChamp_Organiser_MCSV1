Tabs.throneEnhanceStats = {
    tabOrder: 300,
    tabLabel: 'Enhance Stats TR',
    tabDisabled: false,
    myDiv: null,

    init: function (div) {
        var t = Tabs.throneEnhanceStats;
        t.myDiv = div;
        t.buildDisplay();
    },

    buildDisplay: function () {
        var t = Tabs.throneEnhanceStats;
        var m = '<div style=" position: static; width: ' + OrganizerWindowWidth + '; height: ' + OrganizerWindowHeight + '; overflow-x: hidden; overflow-y: auto;" >';
        m += '<DIV id=trStatsMain class=trStat>THRONE ENHANCEMENT STATISTICS</div>';
        m += '<DIV id= trStatsTabDiv2 align=left style="margin-top:10px; overflow-x: auto; overflow-y: auto;position: static;width: 100%;">';
        m += '<TABLE class=trStatTab align=center cellspacing=0>';

        m += '<TR valign=top align=center><TH colspan=' + (OrganizerMaxTRQuality + 2) + '>Enhancing Numbers  (successes/failures)</TH></TR>';

        var qstrings = new Array(uW.g_js_strings.throneRoom.simple, uW.g_js_strings.throneRoom.common, uW.g_js_strings.throneRoom.uncommon,
                    uW.g_js_strings.throneRoom.rare, uW.g_js_strings.throneRoom.epic, uW.g_js_strings.throneRoom.wondrous, uW.g_js_strings.throneRoom.miraculous);


        m += '<TR valign=top align=center><th></th>';
        for (q = 0; q <= OrganizerMaxTRQuality; q++) {
            m += "<td style='font-weight: bold;' class='td" + (q % 2 + 1) + "'  >";
            m += qstrings[q] + "&nbsp;";
            m += '</td>';
        }

        m += '</tr>';


        var st = [0, 0, 0, 0, 0, 0, 0];
        var ft = [0, 0, 0, 0, 0, 0, 0];

        for (l = 0; l <= OrganizerMaxTRLevel; l++) {
            m += '<TR valign=top align=center>';
            m += "<th>";
            if (l != 0) m += "+";
            m += l + '</th>';
            for (q = 0; q < (OrganizerMaxTRQuality+1); q++) {
                if (TRupgradeStats.enhanceSuccess[q][l] == null) TRupgradeStats.enhanceSuccess[q][l] = 0;
                if (TRupgradeStats.enhanceFailure[q][l] == null) TRupgradeStats.enhanceFailure[q][l] = 0;
                st[q] += TRupgradeStats.enhanceSuccess[q][l];
                ft[q] += TRupgradeStats.enhanceFailure[q][l];

                m += "<td class='td" + (q % 2) + "'  >";
                m += TRupgradeStats.enhanceSuccess[q][l] + " / " + TRupgradeStats.enhanceFailure[q][l];
                m += "</td>";
            }
            m += '</TR>';
        }

        m += '<TR valign=top align=center><th> Totals: </th>';
        for (q = 0; q < (OrganizerMaxTRQuality+1); q++) {
            m += "<td style='font-weight: bold;' class='td" + (q % 2) + "'  >";
            m += st[q] + " / " + ft[q];
            m += "</td>";
        }
        m += '</TR>';

        m += '<TR valign=top align=center><th> Percents: </th>';
        for (q = 0; q < (OrganizerMaxTRQuality+1); q++) {
            m += "<td style='font-weight: bold;' class='td" + (q % 2) + "'  >";
            if ((st[q] + ft[q]) == 0)
                m += "--";
            else {
                var p = (100 * st[q] / (st[q] + ft[q]));
                m += p.toFixed(2) + "%";
            }
            m += "</td>";
        }
        m += '</TR>';

        m += '</TABLE></DIV></DIV>';

        m += "<div id='trStats'></div>";
        t.myDiv.innerHTML = m;
    },

    show: function () {
        var t = Tabs.throneEnhanceStats;
        t.buildDisplay();
    },

    hide: function () {
    }
}

