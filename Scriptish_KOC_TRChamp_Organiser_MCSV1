        $("#selectAllQualities").click(function () {
            for (q in t.qualities) {
                var ch = document.getElementById("trQuality" + t.qualities[q]);
                ch.checked = true;
            }
            var ch = document.getElementById("trUnique");
            ch.checked = true;
            t.show();
        });


        $("#unselectAllTypes").click(function () {
