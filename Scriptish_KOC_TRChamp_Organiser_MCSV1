                var champ_id = $(this).attr("id").split("chCardItem")[1];
                ShowChampTooltip($(this), "chtrTooltip", champ_id);
            });

            $(".chOrganizerCardDisp").mouseout(function (A) {
                HideChampTooltip("chtrTooltip");
            });
        }
        // add the large tooltip

        /*
