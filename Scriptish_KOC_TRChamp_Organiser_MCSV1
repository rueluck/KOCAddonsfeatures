        m += '</td><td colspan=3 width=450px>';
        for (a = 1; a <= Seed.throne.slotNum; a++) {
            m += '<a style="font-family: serif; width: 25px;" class="button20 loadGPreset"  id=trPresetNum' + a + ' > <span> ' + a + ' </span></a>';
