function ShowChampTooltip(divToAttachTo, divName, champId) {
    var custom_div = document.createElement('div');
    $(custom_div).html(CustomChampTooltip(champId)).addClass("chtrTooltip").attr('id', divName);
    divToAttachTo.after(custom_div);
}

function HideChampTooltip(divName) {
    var custom_div = document.getElementById(divName);
    custom_div.parentNode.removeChild(custom_div);
}

function ShowThroneTooltip(divToAttachTo, divName, throneId) {
    var custom_div = document.createElement('div');
    $(custom_div).html(CustomThroneTooltip(throneId)).addClass("chtrTooltip").attr('id', divName);
    divToAttachTo.after(custom_div);
}

function HideThroneTooltip(divName) {
    var custom_div = document.getElementById(divName);
    custom_div.parentNode.removeChild(custom_div);
}

function CustomThroneTooltip(trId) {
    if (GeneralOptions.compactView) {
        var t = Tabs.throneOrganizerCompact;
    } else {
        var t = Tabs.throneOrganizer;
    }
    var D = [];
    var w = CM.thronestats.mightByQuality;
    var z = CM.thronestats.mightByLevel;

    var text_only = false;

    if (trId == null) return "<div></div>";

    var throne_item = unsafeWindow.kocThroneItems[trId];

    if (throne_item == null || !throne_item) return;

    D.push("<div class='trCardDisp' id='trCardItem" + throne_item.id + "' style='border: 1px solid white; clear: both;  overflow: visible; position: relative; left: 0px; top: 0px; -moz-transform: scale(" + OrganizerTooltipCardScale + ", " + OrganizerTooltipCardScale + "); -moz-transform-origin: 0% 0%;  -webkit-transform: scale(" + OrganizerTooltipCardScale + ", " + OrganizerTooltipCardScale + ");  -webkit-transform-origin: 0% 0%;);'>");
    if (throne_item.isBroken) {
        D.push(" <div class='cardOverlay semi_transparent rot45'> Broken </div>");
    }
    D.push(" <div class='trCard' style='white-space: normal; padding: 0px;'> ");
    D.push("<div class='section' style='overflow: visible;' id = 'idsection'>");
    D.push(" <div class='title " + throne_item.createPrefix().toLowerCase() + "' style='text-transform: capitalize;'> ");
    D.push(throne_item.name);
    if (!text_only) D.push(" [" + throne_item.id + "]");
    D.push(" </div> ");
    D.push(" <div class='description'> ");
    if (!text_only) {

        D.push(" <div class='portrait " + throne_item.faction + " " + throne_item.type + "'> </div> ");
        D.push("<ul>");
        D.push("<li> " + uW.g_js_strings.commonstr.faction + ": " + throne_item.faction + "</li>");
        D.push("<li> " + uW.g_js_strings.commonstr.quality + ": " + throne_item.createPrefix() + "</li>");
        D.push("<li> " + uW.g_js_strings.commonstr.type + ": " + throne_item.type + "</li>");
        D.push("<li> " + uW.g_js_strings.commonstr.level + ": " + throne_item.level + "</li>");
        D.push("<li> " + uW.g_js_strings.commonstr.might + ": " + unsafeWindow.cm.ThroneView.getMightBonus(throne_item) + "</li>");
        D.push("</ul>");
        D.push(" </div> ");
        D.push(" <ul> ");
    }

    for (M in throne_item.effects) {
        try {
            var N = throne_item.effects[M];
            effect = CM.thronestats.effects[N.id];

            tier = CM.thronestats.tiers[N.id][N.tier];
            if (!tier) tier = CM.thronestats.tiers[N.id][N.tier - 1];

            var base = tier.base || 0;
            var level = throne_item.level || 0;
            var growth = tier.growth || 0;

            percent = +(base + ((level * level + level) * growth * 0.5));

            var wholeNumber = false;
            if (Math.round(parseFloat(percent)) == parseFloat(percent)) wholeNumber = true;

            percent = (percent > 0) ? "+" + percent : +percent;

            if (wholeNumber)
                percent = parseFloat(percent).toFixed(0);
            else
                percent = parseFloat(percent).toFixed(2);
            // percent=Math.ceil(percent);
            css = (M % 2 === 0) ? "even" : "odd";
            B = +(M.split("slot")[1]);
            if (B <= throne_item.quality) {
                D.push(" <li class='effect " + css + "'> " + percent + "% " + effect[1] + " </li> ");
            } else {
                D.push(" <li class='effect disabled " + css + "'> " + percent + "% " + effect[1] + " </li> ");
            }
        }
        catch (e) { }

    }
    D.push(" </ul> ");
    D.push(" </div> ");
    D.push(" </ul> ");
    D.push(" </div> ");
    D.push(" </div> ");
    return D.join("");    
}

function CustomChampTooltip(chId) {
    var T = false;
    var e = function (V, R) {
        var U = +(V.amount.toFixed(2)),
                T = V.name,
                S = (V.id >= 200) ? " statChamp" : " statTroop",
                W = R ? " disabled" : "";
        return '<ul class="effect' + S + W + '">' + U + " " + T + "</ul>"
    };
    if (chId == null) {
        return "<div></div>";
    }

    var champ_item = unsafeWindow.kocChampionItems[chId];
    var effects = champ_item.effects;
    var V = Boolean(T) ? '<span class="icon ' + /*W +*/'"></span>' : "";

    var X = '<table><tr><TD class="chcard2" style="overflow: visible;  width:auto; height: 150px;" id="' + chId + '" >';
    X += '<div class="ChampionX" id="chCardItem"' + chId + '><div class="chbox" style="float: left;"><div class="item_section">';
    X += '<div class="item_titleTT">' + champ_item.name + V + '</div><div class="divider"></div>';

    var quality = (+champ_item.unique > 0) ? unsafeWindow.g_js_strings.throneRoom[CM.CHAMPION.getRarityClasses(6)] : unsafeWindow.g_js_strings.throneRoom[CM.CHAMPION.getRarityClasses(champ_item.rarity)];

    X += '<div class="champ_item_hoverTT" style="background: url(https://kabam1-a.akamaihd.net/kingdomsofcamelot/fb/e2/src/img/champion_hall/';
    X += champ_item.getSrc("70") + ') top left no-repeat;"></div>';
    X += '<div class="stats_item_hover">';
    X += unsafeWindow.g_js_strings.commonstr.faction + ": " + unsafeWindow.g_js_strings.commonstr[CM.CHAMPION.getFactionClasses(champ_item.faction)] + "<br>";
    X += unsafeWindow.g_js_strings.commonstr.quality + ": " + quality + "<br>";
    X += unsafeWindow.g_js_strings.commonstr.type + ": " + unsafeWindow.g_js_strings.champ[CM.CHAMPION.getEquipmentNames(champ_item.type)] + "<br>";
    X += unsafeWindow.g_js_strings.commonstr.level + ": " + champ_item.level + "<br>";
    X += unsafeWindow.g_js_strings.commonstr.might + ": " + champ_item.might + '</div>';
    X += '<div class="clear"></div><div class="divider"></div><ul class="effectsTT">';
    $.each(effects, function (ac, aa) {
        var ab = CM.ChampionManager.getEffect(aa, champ_item.level);
        X += e(ab, ac > +champ_item.rarity)
    });
    X += "</ul></div></div></div>";
    X += '</TD></tr></table>';
    return X;
}

function BuildChampCardGraphical(champ_item) {
//    var t = Tabs.champOrganizerCompact;

    var T = false;
    var c = CM.CHAMPION;
    var e = function (V, R) {
        var U = +(V.amount.toFixed(2)),
                T = V.name,
                S = (V.id >= 200) ? " statChamp" : " statTroop",
                W = R ? " disabled" : "";
        return '<ul class="effect' + S + W + '">' + U + " " + T + "</ul>"
    };

    if (champ_item == null) {
        return "<div></div>";
    }

//        if (typeof champ_item !== "object") {
//            champ_item = kocChampionItems[champ_item]
//        }

    var UW = unsafeWindow;

    //var R = parseInt(CM.ChampionManager.getChampion().id) == parseInt(Z.equippedTo),
    //W = !R ? " otherEquipped" : " mineEquipped",

    var S = champ_item.effects;
    var V = Boolean(T) ? '<span class="icon ' + /*W +*/'"></span>' : "";
    var X = '<div class="ChampionX" id="chCardItem' + champ_item.equipmentId + '"><div class="chbox" style="float: left;">';
    X += '<div class="item_section"><div class="item_title">' + champ_item.name + V + '</div><div class="divider"></div>';

    var U = (+champ_item.unique > 0) ? UW.g_js_strings.throneRoom[c.getRarityClasses(6)] : UW.g_js_strings.throneRoom[c.getRarityClasses(champ_item.rarity)];

    X += '<div class="champ_item_hover" style="background-size: 50% 50%; background: url(https://kabam1-a.akamaihd.net/kingdomsofcamelot/fb/e2/src/img/champion_hall/';
    X += champ_item.getSrc("70") + ') top left no-repeat;"></div>';
    X += '<div class="stats_item_hover">';
    X += UW.g_js_strings.commonstr.faction + ": " + UW.g_js_strings.commonstr[c.getFactionClasses(champ_item.faction)] + "<br>";
    X += UW.g_js_strings.commonstr.quality + ": " + U + "<br>";
    X += UW.g_js_strings.commonstr.type + ": " + UW.g_js_strings.champ[c.getEquipmentNames(champ_item.type)] + "<br>";
    X += UW.g_js_strings.commonstr.level + ": " + champ_item.level + "<br>";
    X += UW.g_js_strings.commonstr.might + ": " + champ_item.might;
    X += '</div>';
    X += '<div class="clear"></div><div class="divider"></div><ul class="effects">';
    $.each(S, function (ac, aa) {
        var ab = CM.ChampionManager.getEffect(aa, champ_item.level);
        X += e(ab, ac > +champ_item.rarity)
    });
    X += "</ul></div></div></div>";
    return X;
}

function BuildChampCardTextual(champ_item, divId, divClass) {

    if (champ_item == null) return "<div></div>";

    var D = [];

    D.push("<div class='" + divClass + "' id='" + divId + champ_item.equipmentId + "'>");
    D.push("<div class='chCard2' style='white-space: normal; padding: 0px;'>");
    D.push("<div class='chCardDetail'>");
    D.push("<ul>");
    if (champ_item.status == CM.CHAMPION.STATUS_BROKEN_UPGRADE || champ_item.status == CM.CHAMPION.STATUS_BROKEN_ENHANCE) { D.push("<li>BROKEN</li>"); }
    // faction/type
    D.push("<li>" + CM.CHAMPION.getFactionClasses(champ_item.faction).toLowerCase() + " / " + CM.CHAMPION.getEquipmentNames(champ_item.type).toLowerCase() + "</li>");
    // quality/level
    var quality = (champ_item.unique > 0) ? "unique" : CM.CHAMPION.getRarityClasses(champ_item.rarity).toLowerCase();
    D.push("<li>" + quality + " / +" + champ_item.level + "</li>");

    for (var slotNumber = 1; slotNumber < OrganizerMaxCHQuality + 1; slotNumber++) {
        var effectLine = champ_item.effects[slotNumber];
        var effect = CM.ChampionManager.getEffect(effectLine, champ_item.level);
        D.push("<li>" + effect.amount + " " + effect.name + "</li>");
    }
    D.push("</ul>");
    D.push("</div>");
    D.push("</div>");
    D.push("</div>");
    return D.join("");
}

function BuildThroneCardGraphical(throne_item, divId, divClass) {
    var D = [];
    var w = CM.thronestats.mightByQuality;
    var z = CM.thronestats.mightByLevel;

    var text_only = false;

    if (throne_item == null) {
        D.push("<div>");
        D.push("</div>");
        return D.join("");
    }

    D.push("<div class='" + divClass + "' id='" + divId + throne_item.id + "' style='overflow: hidden; position: relative; left: 0px; top: 0px; -moz-transform: scale(" + OrganizerCardScale + ", " + OrganizerCardScale + "); -moz-transform-origin: 0% 0%;  -webkit-transform: scale(" + OrganizerCardScale + ", " + OrganizerCardScale + ");  -webkit-transform-origin: 0% 0%;'>");
    if (throne_item.isBroken) {
        D.push(" <div class='cardOverlay semi_transparent rot45'> Broken </div>");
    }
    D.push(" <div class='trCard' style='white-space: normal; padding: 0px;'> ");
    D.push("<div class='section' style='overflow: visible;' id = 'idsection'>");
    D.push(" <div class='title " + throne_item.createPrefix().toLowerCase() + "' style='text-transform: capitalize;'> ");
    D.push(throne_item.name + (throne_item.unique ? " +" + throne_item.level : ""));
    D.push(" </div> ");
    D.push(" <div class='description'> ");
    if (!text_only) {

        var uniquestyle = "";
        if (throne_item.unique > 30000) {
            uniquestyle = 'background:transparent url("https://kabam1-a.akamaihd.net/silooneofcamelot/fb/e2/src/img/throne/icons/70/'+throne_item.faction+'_'+throne_item.type+'_unique_'+throne_item.unique + '.png"); top left no-repeat; background-size: 70px 70px;';
            if (throne_item.unique == 30262 || throne_item.unique == 30264 || throne_item.unique == 30266) { uniquestyle = 'background:transparent url("https://kabam1-a.akamaihd.net/silooneofcamelot/fb/e2/src/img/throne/icons/70/christmas_advisor_normal_1.png"); top left no-repeat; background-size: 70px 70px;';};
            if (throne_item.unique == 30261 || throne_item.unique == 30263 || throne_item.unique == 30265) { uniquestyle = 'background:transparent url("https://kabam1-a.akamaihd.net/silooneofcamelot/fb/e2/src/img/throne/icons/70/christmas_candelabrum_normal_1.png"); top left no-repeat; background-size: 70px 70px;';};
            if (throne_item.unique == 30230 || throne_item.unique == 30240 || throne_item.unique == 30250) { uniquestyle = 'background:transparent url("https://kabam1-a.akamaihd.net/silooneofcamelot/fb/e2/src/img/throne/icons/70/halloween_table_normal_1.png"); top left no-repeat; background-size: 70px 70px;';};
            if (throne_item.unique == 30231 || throne_item.unique == 30241 || throne_item.unique == 30251) { uniquestyle = 'background:transparent url("https://kabam1-a.akamaihd.net/silooneofcamelot/fb/e2/src/img/throne/icons/70/halloween_chair_normal_1.png"); top left no-repeat; background-size: 70px 70px;';};
        }
        D.push("<div class='portrait " + throne_item.faction + " " + throne_item.type + "' style='"+uniquestyle+"'> </div> ");
        D.push("<ul>");
        D.push("<li> " + uW.g_js_strings.commonstr.faction + ": " + throne_item.faction + "</li>");
        D.push("<li> " + uW.g_js_strings.commonstr.quality + ": " + throne_item.createPrefix() + "</li>");
        D.push("<li> " + uW.g_js_strings.commonstr.type + ": " + throne_item.type + "</li>");
        D.push("<li> " + uW.g_js_strings.commonstr.level + ": " + throne_item.level + "</li>");
        D.push("<li> " + uW.g_js_strings.commonstr.might + ": " + unsafeWindow.cm.ThroneView.getMightBonus(throne_item) + "</li>");
        D.push("</ul>");
        D.push(" </div> ");
        D.push(" <ul> ");
    }

    for (slot in throne_item.effects) {
        try {
            var N = throne_item.effects[slot];
            effect = CM.thronestats.effects[N.id];

            tier = CM.thronestats.tiers[N.id][N.tier];
            if (!tier) tier = CM.thronestats.tiers[N.id][N.tier - 1];

            var base = tier.base || 0;
            var level = throne_item.level || 0;
            var growth = tier.growth || 0;

            if (slot == 'slot6') {  //if it has a slot 6, it automatically has a jewel
                JewelQuality = throne_item["effects"]['slot6'].quality;
                GrowthLimit = unsafeWindow.cm.thronestats.jewelGrowthLimit[JewelQuality];
                if (GrowthLimit <= level) level = GrowthLimit
            }

            percent = +(base + ((level * level + level) * growth * 0.5));

            var wholeNumber = false;
            if (Math.round(parseFloat(percent)) == parseFloat(percent)) wholeNumber = true;

            percent = (percent > 0) ? "+" + percent : +percent;

            if (wholeNumber)
                percent = parseFloat(percent).toFixed(0);
            else
                percent = parseFloat(percent).toFixed(2);

            css = (slot % 2 === 0) ? "even" : "odd";
            B = +(slot.split("slot")[1]);

            if (B <= throne_item.quality) {
                D.push(" <li class='effect " + css + "'> " + percent + "% " + effect[1] + " </li> ");
            } else {
                D.push(" <li class='effect disabled " + css + "'> " + percent + "% " + effect[1] + " </li> ");
            }
        }
        catch (e) { }

    }
    D.push(" </ul> ");
    D.push(" </div> ");
    D.push(" </ul> ");
    D.push(" </div> ");
    D.push(" </div> ");
    return D.join("");
}


function BuildThroneCardTextual(throne_item, divId, divClass) {  //divClass example 'trCardDisp'

    if (throne_item == null) return "<div></div>";

    var D = [];

    D.push("<div class='" + divClass + "' id='" + divId + throne_item.id + "'>");
        D.push("<div class='trCard2' style='white-space: normal; padding: 0px;'>");
            D.push("<div id='trCardDetail' class='trCardDetail'>");
            D.push("<ul>");
            if (throne_item.isBroken) { D.push("<li>BROKEN</li>"); }
            var faction = throne_item.faction.toLowerCase();
            var quality = throne_item.createPrefix().toLowerCase();
            var cardType = throne_item.type + " of " + throne_item.createSuffix().toLowerCase();
            if (throne_item.unique) cardType = throne_item.name.toLowerCase();
                D.push("<li>" + faction + " / " + cardType + "</li>");
                D.push("<li>" + quality + " / +" + throne_item.level + "</li>");
                for (slot in throne_item.effects) {
                    try {
                        var N = throne_item.effects[slot];
                        effect = CM.thronestats.effects[N.id];

                        tier = CM.thronestats.tiers[N.id][N.tier];
                        if (!tier) tier = CM.thronestats.tiers[N.id][N.tier - 1];

                        var base = tier.base || 0;
                        var level = throne_item.level || 0;
                        var growth = tier.growth || 0;

                        if (slot == 'slot6') {  //if it has a slot 6, it automatically has a jewel
                            JewelQuality = throne_item["effects"]['slot6'].quality;
                            GrowthLimit = unsafeWindow.cm.thronestats.jewelGrowthLimit[JewelQuality];
                            if (GrowthLimit <= level) level = GrowthLimit
                        }

                        percent = +(base + ((level * level + level) * growth * 0.5));

                        var wholeNumber = false;
                        if (Math.round(parseFloat(percent)) == parseFloat(percent)) wholeNumber = true;

                        percent = (percent > 0) ? "+" + percent : +percent;

                        if (wholeNumber)
                            percent = parseFloat(percent).toFixed(0);
                        else
                            percent = parseFloat(percent).toFixed(2);

                        css = (slot % 2 === 0) ? "even" : "odd";
                        B = +(slot.split("slot")[1]);

                        effect[1] = effect[1];
                        
                        if (B <= throne_item.quality) {
                            D.push(" <li class='effect " + css + "'> " + percent + "% " + effect[1] + " </li> ");
                        } else {
                            D.push(" <li class='effect disabled " + css + "'> " + percent + "% " + effect[1] + " </li> ");
                        }
                    }
                    catch (e) { }

                }
                D.push(" </ul> ");
            D.push(" </div> ");
        D.push(" </div> ");
    D.push(" </div> ");
    return D.join("");
}

function getChampItemStats(chId, separater) {

    separater = separater || "    ";

    var champ_item = unsafeWindow.kocChampionItems[chId];
    
    var D = [];

    D.push(champ_item.name);

    D.push("Faction: " + unsafeWindow.cm.CHAMPION.getFactionClasses(champ_item.faction).capitalize());
    var quality = (champ_item.unique > 0) ? "Unique" : unsafeWindow.cm.CHAMPION.getRarityClasses(champ_item.rarity);
    D.push("Quality: " + quality.capitalize());
    D.push("Type: " + unsafeWindow.g_js_strings.champ[unsafeWindow.cm.CHAMPION.getEquipmentNames(champ_item.type)]);
    D.push("Level: " + champ_item.level);
    D.push("Might: " + champ_item.might);
    
    for (var slotNumber = 1; slotNumber < OrganizerMaxCHQuality + 1; slotNumber++) {
        var effectLine = champ_item.effects[slotNumber];
        var effect = unsafeWindow.cm.ChampionManager.getEffect(effectLine, champ_item.level);
        D.push("Row " + slotNumber + ": " + effect.amount + " " + effect.name);
    }

    var cText = D.join(separater);

    if (separater == "||") cText = ":::. |" + cText;

    return cText;
}

function getThroneItemStats(trId, separater) {

    separater = separater || "    ";

    var throne_item = unsafeWindow.kocThroneItems[trId];

    if (throne_item == null || !throne_item) return "";

    var D = [];

    D.push(throne_item.name);
    D.push("Faction: " + throne_item.faction);
    D.push("Quality: " + throne_item.createPrefix());
    D.push("Type: " + throne_item.type);
    D.push("Level: " + throne_item.level);
    D.push("Might: " + unsafeWindow.cm.ThroneView.getMightBonus(throne_item));
    for (slot in throne_item.effects) {
        try {
            var N = throne_item.effects[slot];
            effect = CM.thronestats.effects[N.id];

            tier = CM.thronestats.tiers[N.id][N.tier];
            if (!tier) tier = CM.thronestats.tiers[N.id][N.tier - 1];

            var base = tier.base || 0;
            var level = throne_item.level || 0;
            var growth = tier.growth || 0;

            if (slot == 'slot6') {  //if it has a slot 6, it automatically has a jewel
                JewelQuality = throne_item["effects"]['slot6'].quality;
                GrowthLimit = unsafeWindow.cm.thronestats.jewelGrowthLimit[JewelQuality];
                if (GrowthLimit <= level) level = GrowthLimit
            }

            percent = +(base + ((level * level + level) * growth * 0.5));

            var wholeNumber = false;
            if (Math.round(parseFloat(percent)) == parseFloat(percent)) wholeNumber = true;

            percent = (percent > 0) ? "+" + percent : +percent;

            if (wholeNumber)
                percent = parseFloat(percent)           .toFixed(0);
            else
                percent = parseFloat(percent).toFixed(2);

            css = (slot % 2 === 0) ? "even" : "odd";
            B = +(slot.split("slot")[1]);

            effect[1] = effect[1];

            D.push("Row " + B + ": " + percent + "% " + effect[1]);
        }
        catch (e) { }

    }
    var cText = D.join(separater);
    if (separater == "||") cText = ":::. |" + cText;

    return cText;
}

function displayCityAstone() {

    var city = pickAetherUseCity();
    var m = '';
    if (city == -1) {
        m = '<div>No Aetherstone Available</div>';
    } else {
        var aether = parseInt(Seed.resources["city" + Seed.cities[city][0]]["rec5"][0]);
        m = '<div>AETHER: City #' + (city + 1) + ' = ' + addCommas(aether) + '</div>';
    }
    return m;
}

function getChampName( presetIndex ) { //presetIndex is 1 based but needs to be 0 based for champ name so we will minus 1
    var thisChampion = unsafeWindow.seed.champion.champions[presetIndex-1];
    return thisChampion.name;
}

function getChampID( presetIndex ) { //presetIndex is 1 based but needs to be 0 based for champ name so we will minus 1
    var thisChampion = unsafeWindow.seed.champion.champions[presetIndex-1];
    return thisChampion.championId;
}

function postChampSlot ( slot ) {
    var champId = getChampID(slot);
    var D = [];
    for (var chId in unsafeWindow.kocChampionItems) if (unsafeWindow.kocChampionItems[chId].equippedTo == champId) D.push(chId);
    var champStats = GenerateChampPresetEffectsString(D, false);
    D = [];
    D.push("Champ Hall Slot #" + slot);
    D.push(getChampName(slot));
    if (CHpresetData.showCHName) D.push("(" + CHpresetData.presetNames[slot-1] + ")");
    if (CHpresetData.showCHMight) D.push(getChampMight());
    D.push(champStats);
    sendChat(":::. |" + D.join("||"));
}

function postChampPreset ( presetIndex ) {
    var chP = getPresetCHObject(parseInt(presetIndex));
    var D = [];
    for (var p in chP) D.push(p);
    var champStats = GenerateChampPresetEffectsString(D, false);
    D = [];
    D.push("Champ Hall Tag #" + presetIndex);
    D.push(getChampName(presetIndex));
    if (CHpresetData.showCHName) D.push("(" + CHpresetData.presetNames[presetIndex-1] + ")");
    if (CHpresetData.showCHMight) D.push(getChampMight());
    D.push(champStats);
    sendChat(":::. |" + D.join("||"));
}


function postThroneSlot( slot ) {
    var throneStats = GenerateThronePresetEffectsString(Seed.throne.slotEquip[slot], false);
    var D = [];
    D.push("Throne Room Slot #" + slot);
    if (TRpresetData.showTRName) D.push("(" + TRpresetData.presetNames[slot] + ")");
    if (TRpresetData.showTRMight) D.push("TR Might: " + getThroneMight());
    D.push(throneStats);

    sendChat(":::. |" + D.join("||"));
}

function postThronePreset( presetIndex ) {
    var trP = getPresetTRObject(parseInt(presetIndex));
    var D = [];
    for (var p in trP) D.push(p);
    var throneStats = GenerateThronePresetEffectsString(D, false);
    D = [];
    D.push("Throne Room Tag #" + presetIndex);
    if (TRpresetData.showTRName) D.push("(" + TRpresetData.presetNames[presetIndex] + ")");
    if (TRpresetData.showTRMight) D.push("TR Might: " + getThroneMight());
    D.push(throneStats);

    sendChat(":::. |" + D.join("||"));
}

function getChampBrokeMight() {
    var totMight = 0;
    for (chId in unsafeWindow.kocChampionItems) {
        var champ_item = unsafeWindow.kocChampionItems[chId];
        if (champ_item.status == 1) continue;
        var q = champ_item.rarity;
        if (champ_item.unique != 0) q -= 1;
        var l = champ_item.level;
        totMight += unsafeWindow.cm.thronestats.mightByQuality[q].Might + unsafeWindow.cm.thronestats.mightByLevel[l].Might;
    }
    return "<b>You have approx " + addCommas(totMight) + " in broken champ might</b>";
}

function getThroneMight() {
    var totMight = 0;
    for (trId in unsafeWindow.kocThroneItems) {
        var throne_item = unsafeWindow.kocThroneItems[trId];
        if (throne_item == null || !throne_item) continue;
        totMight += unsafeWindow.cm.ThroneView.getMightBonus(throne_item);
    }
    return addCommas(totMight);
}

function getThroneBrokeMight() {
    var totMight = 0;
    for (trId in unsafeWindow.kocThroneItems) {
        var throne_item = unsafeWindow.kocThroneItems[trId];
        if (throne_item == null || !throne_item) continue;
        if (throne_item.isBroken) totMight += unsafeWindow.cm.ThroneView.getMightBonus(throne_item);
    }
    return "<b>You have " + addCommas(totMight) + " in broken throne might</b>";
}

function getChampMight() {
    var totMight = 0;
    for (chId in unsafeWindow.kocChampionItems) {
        var champ_item = unsafeWindow.kocChampionItems[chId];
        var q = champ_item.rarity;
        if (champ_item.unique != 0) q -= 1;
        var l = champ_item.level;
        totMight += unsafeWindow.cm.thronestats.mightByQuality[q].Might + unsafeWindow.cm.thronestats.mightByLevel[l].Might;
    }
    return "Approx Might: " + addCommas(totMight);
}


function isAnyTRBroke() {
    for (trId in unsafeWindow.kocThroneItems) {
        var throne_item = unsafeWindow.kocThroneItems[trId];
        if (throne_item == null || !throne_item) return false;
        if (throne_item.isBroken) return true;
    }
    return false;
}


function repairTimeToText(total_time) {
    var secondsInMinutes = 60;
    var secondsInHours = 3600;
    var secondsInDays = 43200;

    var totalDays = 0;
    var totalHours = 0;
    var totalMinutes = 0;

    if (total_time > secondsInDays) {
        totalDays = (total_time - (total_time % secondsInDays)) / secondsInDays;
        total_time = total_time % secondsInDays;
    }
    if (total_time > secondsInHours) {
        totalHours = (total_time - (total_time % secondsInHours)) / secondsInHours;
        total_time = total_time % secondsInHours;
    }
    if (total_time > secondsInMinutes) {
        totalMinutes = (total_time - (total_time % secondsInMinutes)) / secondsInMinutes;
        total_time = total_time % secondsInMinutes;
    }
    var totalSeconds = total_time;

    return (totalDays + " Days " + totalHours + " Hours " + totalMinutes + " Minutes " + totalSeconds + " Seconds");
}


function CustomChampContextMenu(id, top, left, fromOrganizer, fromChampItem) {

    var champ_item = unsafeWindow.kocChampionItems[id];
    var tempDiv = document.getElementById("contextMenu");
    if (tempDiv) tempDiv.remove();

    var dDiv = document.createElement('div');
    dDiv.id = "contextMenu";
    dDiv.style.left = left + "px";
    dDiv.style.top = top + "px";

    dDiv.addEventListener("mouseleave", function () {
        $(this).find('buttonv2').remove();
        $(this).find('context_menu_title').remove();
        $(this).remove();
    }, false);

    var dDiv2 = document.createElement('div');
    dDiv2.className = "context_menu_title";
    dDiv2.innerHTML = "<b>" + champ_item.name + "</b>";
    dDiv.appendChild(dDiv2);

    if (fromOrganizer) {
        document.body.appendChild(dDiv);
    } else {
        var cmm = document.getElementsByClassName("cmModalContainer")[0];
        cmm.appendChild(dDiv);
    }

    DoChampContextMenu(id, fromOrganizer, fromChampItem);

}

function DoChampContextMenu(id, fromOrganizer, fromChampItem) {

    var champ_item = unsafeWindow.kocChampionItems[id];

    if (!champ_item) return;

    var idx = 1;

    if (!fromOrganizer) {
        for (i = 0; i < 4; i++) {
            var champClass = $(".champion_states").children(".name" + i).attr('class');
            if ( champClass.indexOf("active") == -1) continue;
            idx = i + 1;
            break;
        }
    }
   
    //            var btn = document.createElement('a');
    //            $(btn).addClass("buttonv2 box green")
    //            .html("TEST LINE 1")
    //            .css('color', 'yellow')
    //            .attr('box_num', 10)
    //            .bind("mouseover", function () {
    //                ShowChampTooltip($(this), "testDiv", id);
    //            })
    //            .bind("mouseout", function () {
    //                HideChampTooltip("testDiv");
    //            });
    //            $("#contextMenu div.context_menu_title").after(btn);

    if (fromChampItem) {

        var btn4 = document.createElement('a');
        $(btn4).addClass("buttonv2 red box")
            .html("Salvage")
            .css('color', 'white')
            .attr('box_num', 10)
            .bind("click", function () {
                if (confirm('Are you sure you want to salvage this item?')) SalvageChampItem(id);
                $("#contextMenu").remove();
            });
        $("#contextMenu div.context_menu_title").after(btn4);

        var btn4 = document.createElement('a');
        $(btn4).addClass("buttonv2 brown box")
            .html("Upgrade")
            .css('color', 'white')
            .attr('box_num', 10)
            .bind("click", function () {
                CM.ChampionModalView.upgEnhSlideIn(id, "upg");
                $("#contextMenu").remove();
            });
        $("#contextMenu div.context_menu_title").after(btn4);

        var btn4 = document.createElement('a');
        $(btn4).addClass("buttonv2 brown box")
            .html("Enhance")
            .css('color', 'white')
            .attr('box_num', 10)
            .bind("click", function () {
                CM.ChampionModalView.upgEnhSlideIn(id, "enh");
                $("#contextMenu").remove();
            });
        $("#contextMenu div.context_menu_title").after(btn4);

        if (champ_item.equippedTo) {
            var btn4 = document.createElement('a');
            $(btn4).addClass("buttonv2 blue box")
                .html("Unequip")
                .css('color', 'white')
                .attr('box_num', 10)
                .bind("click", function () {
                    Tabs.champPresets.unequipItem(id);
                    $("#contextMenu").remove();
                });
            $("#contextMenu div.context_menu_title").after(btn4);
        } else {
            var btn4 = document.createElement('a');
            $(btn4).addClass("buttonv2 blue box")
                .html("Equip")
                .css('color', 'white')
                .attr('box_num', 10)
                .bind("click", function () {
                    Tabs.champPresets.equipItem(id);
                    $("#contextMenu").remove();
                });
            $("#contextMenu div.context_menu_title").after(btn4);
        }

    }

    var btn4 = document.createElement('a');
    $(btn4).addClass("buttonv2 blue box")
        .html("Unequip All")
        .css('color', 'white')
        .attr('box_num', 10)
        .bind("click", function () {
            if (confirm('Are you sure you want to unequip all items?')) Tabs.champPresets.unequipAll(idx);
            $("#contextMenu").remove();
        });
    $("#contextMenu div.context_menu_title").after(btn4);

    // create a button to "remove all tags"
    if (getObjectCollectionCount(CHpresetData.taggedItems) > 0) {
        var btn4 = document.createElement('a');
        $(btn4).addClass("buttonv2 brown box")
            .html("Remove All Tags")
            .css('color', 'white')
            .attr('box_num', 10)
            .bind("click", function () {
                Tabs.champPresets.removeAllTagItems();
                $("#contextMenu").remove();
            });
        $("#contextMenu div.context_menu_title").after(btn4);
    }

    btn = document.createElement('a');
    $(btn).addClass("buttonv2 red box")
        .html("Load To Preview CH")
        .css('color', 'white')
        .attr('box_num', 15)
        .bind("click", function () {
            Tabs.champPreview.loadCard(id);
            $("#contextMenu").remove();
    });
    $("#contextMenu div.context_menu_title").after(btn);

    // create a button to "tag" an item
    btn = document.createElement('a');
    if (!CHpresetData.taggedItems[id]) {
        $(btn).addClass("buttonv2 brown box")
            .html("Tag Item")
            .css('color', 'white')
            .attr('box_num', 14)
            .bind("click", function () {
                Tabs.champPresets.addTagItem(id);
                $("#contextMenu").remove();
            });
    } else {
        $(btn).addClass("buttonv2 brown box")
            .html("Remove Tag")
            .css('color', 'white')
            .attr('box_num', 10)
            .bind("click", function () {
                Tabs.champPresets.removeTagItem(id);
                $("#contextMenu").remove();
            });
    }
    $("#contextMenu div.context_menu_title").after(btn);

    // create a button to Auto-enhance an item
    if (champ_item.rarity < OrganizerMaxCHQuality) {
        var btn = document.createElement('a');
        $(btn).addClass("buttonv2 brown box")
        .html("Auto Enhance")
        .css('color', 'yellow')
        .attr('box_num', 10)
        .bind("click", function () {
            Tabs.champUpgrader.addEnhanceItem(id);
            Tabs.champUpgrader.repaint();
            $("#contextMenu").remove();
        });
        $("#contextMenu div.context_menu_title").after(btn);
    }

    // create a button to set the item to auto-update
    if (champ_item.level < OrganizerMaxCHLevel) {
        btn = document.createElement('a');
        $(btn).addClass("buttonv2 green box")
        .html("Auto Upgrade")
        .css('color', 'blue')
        .attr('box_num', 11)
        .bind("click", function () {
            Tabs.champUpgrader.addUpgradeItem(id);
            Tabs.champUpgrader.repaint();
            $("#contextMenu").remove();
        });
        $("#contextMenu div.context_menu_title").after(btn);
    }

    if (GeneralOptions.showTabPresetsCH) {
        var btn = document.createElement('a');
        $(btn).addClass("buttonv2 red box")
        .html("Save Preset #" + idx + " Tags")
        .css('color', 'white')
        .attr('box_num', 15)
        .bind("click", function () {
            Tabs.champPresets.addPresetTags(idx);
            Tabs.champPresets.init($("#tabs-presetsCH")[0]);
            $("#contextMenu").remove();
        });
        $("#contextMenu div.context_menu_title").after(btn);

        var presetTagCount = getPresetCHTagCount(idx);

        if (presetTagCount != 0) {
            var btn = document.createElement('a');
            $(btn).addClass("buttonv2 red box")
            .html("Clear Preset #" + idx + " Tags")
            .css('color', 'white')
            .attr('box_num', 10)
            .bind("click", function () {
                Tabs.champPresets.clearPresetTags(idx);
                Tabs.champPresets.init($("#tabs-presetsCH")[0]);
                $("#contextMenu").remove();
            });
            $("#contextMenu div.context_menu_title").after(btn);

            var btn = document.createElement('a');
            $(btn).addClass("buttonv2 red box")
            .html("Equip Tag Preset #" + idx)
            .css('color', 'white')
            .attr('box_num', 10)
            .bind("click", function () {
                Tabs.champPresets.equipPresetTags(idx);
                $("#contextMenu").remove();
            });
            $("#contextMenu div.context_menu_title").after(btn);
        }
    }

    // create a button to copy the stats
    btn = document.createElement('a');
    $(btn).addClass("buttonv2 blue box")
        .html("Copy Stats")
        .css('color', 'white')
        .attr('box_num', 12)
        .bind("click", function () {
            window.prompt("Copy to clipboard: Ctrl+C", getChampItemStats(id));
            $("#contextMenu").remove();
        });
    $("#contextMenu div.context_menu_title").after(btn);

    // create a button to post the stats
    btn = document.createElement('a');
    $(btn).addClass("buttonv2 blue box")
        .html("Post Stats to Chat")
        .css('color', 'white')
        .attr('box_num', 13)
        .bind("click", function () {
            sendChat(getChampItemStats(id, "||"));
            $("#contextMenu").remove();
        });
    $("#contextMenu div.context_menu_title").after(btn);

    // create a button to post the stats
    btn = document.createElement('a');
    $(btn).addClass("buttonv2 blue box")
        .html("Export Excel")
        .css('color', 'white')
        .attr('box_num', 13)
        .bind("click", function () {
            ExportChampToExcel();
            $("#contextMenu").remove();
        });
    $("#contextMenu div.context_menu_title").after(btn);

    //if (champ_item.status != CM.CHAMPION.STATUS_BROKEN_UPGRADE && champ_item.status != CM.CHAMPION.STATUS_BROKEN_ENHANCE) {
    if (champ_item.status == 1) {
        if (!custom) {
            // no salvage on first x items
            if (GeneralOptions.CHsalvageSafety) {

                var keys = unsafeWindow.Object.keys(unsafeWindow.kocChampionItems);
                if (keys.indexOf(id) < GeneralOptions.CHnumSafety && keys.indexOf(id) > -1) {
                    $("#contextMenu a:nth-last-child(2)").remove();
                }

            }

            if (GeneralOptions.noMassSalvage) {
                $("#contextMenu a").last().remove();
            }
        }
    }

}


function SalvageChampItem(id) {
    var t = Tabs.champSalvage;

    var params = unsafeWindow.Object.clone(unsafeWindow.g_ajaxparams);
    var num_city = pickAetherSalvageCity();
    if ( num_city < 0)
    {
        num_city = +GeneralOptions.salvageCityNum;
        logit(" cities full");
        t.setStatus("All cities are (nearly) full of aetherstone");
    }

    params.action = "8";
    params.eids = id;
    params.cityId = Seed.cities[num_city][0];
            
    new AjaxRequest(unsafeWindow.g_ajaxpath + "ajax/ceEquipmentManagerAjax.php" + unsafeWindow.g_ajaxsuffix, {
        method: "post",
        parameters: params,
        loading: true,
        onSuccess: function (transport) {
            try {
                var rslt = eval("(" + transport.responseText + ")");
                var champ_item = unsafeWindow.kocChampionItems[id];
                        
                if(rslt.ok == true) {
                    var eid = rslt.equipmentIds[0];
                    champ_item = unsafeWindow.kocChampionItems[eid];
                            
                    if (champ_item) SalvageLog('Deleted Champion item '+ champ_item.name);
                    CHsalvageData.numSalvagedItems++;
                    CHsalvageData.numSalvagedItems2++;
                    CHsaveSalvageData();

                    if (champ_item) {
                        CHsalvageData.numSalvaged[champ_item.rarity]++;
                        CHsaveSalvageData();
                        t.removeItem(eid , Seed.cities[num_city][0], parseInt(rslt.aetherstones));
                    }
                    t.displayNumberSalvaged();
                }
                else
                {
                    //logit("rslt: " + inspect(rslt,3,1));
                    if (champ_item) t.setStatus('Unable to salvage item ' + champ_item.name);
                }

                var idx = t.delItems.indexOf(id);
                if (idx >=0)
                {
                    t.delItems.splice(idx,1); 
                    // Remove item from array regardless
                    // of success. Catch on next refresh
                }
                if (t.delItems.length > 0) { // Check if the array is empty
                    t.upgradeAndDelete();
                } else {
                    //logit("deletes done");
                    t.deleting = false;
                    t.setStatus('Salvaging complete.  Waiting for next cycle.');
                    return;
                }
            } catch (e) {
                logit("Delete exception: " + inspect(e,3,1));
                t.deleting = false;
            }
        },
        onFailure: function () {
            //logit("failure case");
            t.delIems = [];
            t.deleting = false;
            if (unsafeWindow.kocChampionItems[id] )
                logit("salvage failed for item " + unsafeWindow.kocChampionItems[id].name );
            return;
        }
    });
}

function SalvageThroneItem(id) {

    var t = Tabs.throneSalvage;

    var params = unsafeWindow.Object.clone(unsafeWindow.g_ajaxparams);
    var num_city = pickAetherSalvageCity();
    if (num_city < 0) {
        num_city = +GeneralOptions.salvageCityNum;
        logit(" cities full");
        t.setStatus("All cities are (nearly) full of aetherstone");
    }

    params.ctrl = 'throneRoom\\ThroneRoomServiceAjax';
    params.action = 'salvage';
    params.itemId = id;
    params.cityId = Seed.cities[num_city][0];

    new AjaxRequest(unsafeWindow.g_ajaxpath + "ajax/_dispatch53.php" + unsafeWindow.g_ajaxsuffix, {
        method: "post",
        parameters: params,
        loading: true,
        onSuccess: function (transport) {
            try {
                var rslt = eval("(" + transport.responseText + ")");
                //logit("Salvage: " + inspect(rslt,3,1));
                var throne_item = unsafeWindow.kocThroneItems[id];
                if (rslt.ok) {
                    if (throne_item) SalvageLog('Deleted Throne Room item ' + throne_item.name);
                    TRsalvageData.numSalvagedItems++;
                    TRsalvageData.numSalvagedItems2++;
                    TRsaveSalvageData();


                    if (throne_item) {
                        TRsalvageData.numSalvaged[throne_item.quality]++;
                        TRsaveSalvageData();
                        Tabs.throneSalvage.removeItem(id, Seed.cities[num_city][0], rslt.aetherstones);
                    }

                    Tabs.throneSalvage.displayNumberSalvaged();

                    var sidx = TRsalvageData.upgradedToDelete.indexOf(id);
                    if (sidx >= 0) {
                        TRsalvageData.upgradedToDelete.splice(sidx, 1); // Remove item from array
                        TRsaveSalvageData();
                    }
                }
                else {
                    logit("rslt: " + inspect(rslt, 3, 1));
                    if (throne_item) Tabs.throneSalvage.setStatus('Unable to salvage item ' + throne_item.name);

                    // store off the object id and record the number of failures.  If it fails too many times, skip it next time.
                    //if ( !t.salvageFailList[id] ) 
                    //   t.salvageFailList[id] = 1;
                    //else
                    //   t.salvageFailList[id]++;

                    //logit("Salvage failed.  Current number of failures of item " + id +" is " + t.salvageFailList[id]);

                    unsafeWindow.cm.ThroneView.renderInventory(unsafeWindow.kocThroneItems);
                }

                var idx = t.delItems.indexOf(id);
                if (idx >= 0) {
                    t.delItems.splice(idx, 1);
                    // Remove item from array regardless
                    // of success. Catch on next refresh
                }
                if (t.delItems.length > 0) { // Check if the array is empty
                    t.upgradeAndDelete();
                } else {
                    //logit("deletes done");
                    t.deleting = false;
                    t.setStatus('Salvaging complete.  Waiting for next cycle.');
                    return;
                }
            } catch (e) {
                logit("Delete exception: " + inspect(e, 3, 1));
                t.deleting = false;
            }
        },
        onFailure: function () {
            logit("failure case");
            t.delIems = [];
            t.deleting = false;
            if (unsafeWindow.kocThroneItems[id])
                logit("salvage failed for item " + unsafeWindow.kocThroneItems[id].name);
            unsafeWindow.cm.ThroneView.renderInventory(unsafeWindow.kocThroneItems);
            return;
        }
    });
}

function ExportChampToExcel() {
    var headers = [ "NAME", "FACTION", "QUALITY", "TYPE", "LEVEL", "MIGHT", "ROW 1", "ROW 2", "ROW 3", "ROW 4", "ROW 5"];
    var ExcelTable = document.createElement('table');
    var ExcelBody = document.createElement('tbody');
    var ExcelRow = document.createElement('tr'); 
    var ExcelColumn = "";
    for (i = 0; i < headers.length; i++) {
        ExcelColumn = document.createElement('th');
        ExcelColumn.appendChild(document.createTextNode(headers[i]));
        ExcelRow.appendChild(ExcelColumn);
    }
    ExcelBody.appendChild(ExcelRow);
    
    var columns = [];

    for (chId in unsafeWindow.kocChampionItems) {
        var champ_item = unsafeWindow.kocChampionItems[chId];
        if (!champ_item) continue;
        columns = [];
        columns.push(champ_item.name);
        columns.push(CM.CHAMPION.getFactionClasses(champ_item.faction));
        if (champ_item.unique) {
            columns.push("unique");
        } else {
            columns.push(CM.CHAMPION.getRarityClasses(champ_item.rarity));
        }
        columns.push(CM.CHAMPION.getEquipmentNames(champ_item.type));
        columns.push(champ_item.level);
        columns.push(champ_item.might);
        for (var slotNumber = 1; slotNumber < OrganizerMaxCHQuality + 1; slotNumber++) {
            var effectLine = champ_item.effects[slotNumber];
            var effect = CM.ChampionManager.getEffect(effectLine, champ_item.level);
            columns.push(effect.amount + " " + effect.name);
        }
        columns.reverse();
        ExcelRow = document.createElement('tr');
        while (columns.length > 0) {
            ExcelColumn = document.createElement('td');
            ExcelColumn.appendChild(document.createTextNode(columns.pop()));
            ExcelRow.appendChild(ExcelColumn);
        }
        ExcelBody.appendChild(ExcelRow);
    }
    ExcelTable.appendChild(ExcelBody);

    window.open('data:application/vnd.ms-excel,' + encodeURIComponent(ExcelTable.outerHTML));
}

function ExportThroneToExcel() {
    var headers = [ "NAME", "FACTION", "QUALITY", "TYPE", "LEVEL", "MIGHT", "JEWEL", "ROW 1", "ROW 2", "ROW 3", "ROW 4", "ROW 5", "ROW 6"];
    var ExcelTable = document.createElement('table');
    var ExcelBody = document.createElement('tbody');
    var ExcelRow = document.createElement('tr'); 
    var ExcelColumn = "";
    for (i = 0; i < headers.length; i++) {
        ExcelColumn = document.createElement('th');
        ExcelColumn.appendChild(document.createTextNode(headers[i]));
        ExcelRow.appendChild(ExcelColumn);
    }
    ExcelBody.appendChild(ExcelRow);
    
    var columns = [];

    for (trId in unsafeWindow.kocThroneItems) {
        var throne_item = unsafeWindow.kocThroneItems[trId];
        if (!throne_item) continue;
        columns = [];
        columns.push(throne_item.name);
        columns.push(throne_item.faction);
        columns.push(throne_item.quality);
        columns.push(throne_item.type);
        columns.push(throne_item.level);
        columns.push(unsafeWindow.cm.ThroneView.getMightBonus(throne_item));
        if (throne_item.jewel) {
            var jewel_item = throne_item.jewel;
            var qlty = unsafeWindow.cm.thronestats.jewelGrowthLimit[jewel_item.quality];
            var amt = unsafeWindow.cm.ThroneController.getEffectAmount(jewel_item, qlty);
            var jewelName = unsafeWindow.cm.ThroneController.jewelName(jewel_item);
            if (amt == 0) jewelName = "none";
            columns.push(jewelName);
        } else {
            columns.push("none");
        }
        for (var O in throne_item["effects"]) {
            var slotNumber = +(O.split("slot")[1]);
            var id = throne_item["effects"]["slot" + slotNumber]["id"];
            var tier = parseInt(throne_item["effects"]["slot" + slotNumber]["tier"]);
            var level = throne_item.level;
            p = unsafeWindow.cm.thronestats.tiers[id][tier];
            var effectName = unsafeWindow.cm.thronestats["effects"][id]["1"];
            while (!p && (tier > 0)) { tier--; p = unsafeWindow.cm.thronestats.tiers[id][tier]; }
            if (!p) continue; // can't find stats for tier

            if (slotNumber == 6) {
                JewelQuality = throne_item["effects"]["slot" + slotNumber].quality;
                GrowthLimit = unsafeWindow.cm.thronestats.jewelGrowthLimit[JewelQuality];
                if (GrowthLimit <= level) level = GrowthLimit
            }
            var Current = p.base + ((level * level + level) * p.growth * 0.5);
            columns.push(Current + "% " + effectName);
        }
        
        columns.reverse();
        ExcelRow = document.createElement('tr');
        while (columns.length > 0) {
            ExcelColumn = document.createElement('td');
            ExcelColumn.appendChild(document.createTextNode(columns.pop()));
            ExcelRow.appendChild(ExcelColumn);
        }
        ExcelBody.appendChild(ExcelRow);
    }
    ExcelTable.appendChild(ExcelBody);

    window.open('data:application/vnd.ms-excel,' + encodeURIComponent(ExcelTable.outerHTML));
}


//only run in the main iframe
if (document.location.toString().match('src/main_src.php') ) OrganizerStartup ();
