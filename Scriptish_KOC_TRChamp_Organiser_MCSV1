//                            champ_item = unsafeWindow.kocChampionItems[eid];
//                            
//                            if (champ_item) SalvageLog('Deleted Champion item '+ champ_item.name);
//                            CHsalvageData.numSalvagedItems++;
//                            CHsalvageData.numSalvagedItems2++;
