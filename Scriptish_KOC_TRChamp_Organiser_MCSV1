        m += '<td><input type=checkbox ' + (TRupgradeData.usePS ? "CHECKED" : "") + ' id=trUsePS><div style="white-space:nowrap;display:inline-block;">Protection Stones (<div style="white-space:nowrap;display:inline-block;" id=trUsePSLabel>' + unsafeWindow.ksoItems[20002].count + '</div>)</div></td>';
