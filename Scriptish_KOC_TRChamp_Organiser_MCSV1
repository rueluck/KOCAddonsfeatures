function readGeneralOptions () {
    rel = rel.replace("123", "relocate");
    s = GM_getValue ('GenOptions_' + getServerId());
    if (s != null){
        opts = JSON2.parse (s);
        for (k in opts){
            if (matTypeof(opts[k]) == 'object')
                for (kk in opts[k])
                    GeneralOptions[k][kk] = opts[k][kk];
            else
                GeneralOptions[k] = opts[k];
        }
    }
    GeneralOptions.baconColor = 0;
}

