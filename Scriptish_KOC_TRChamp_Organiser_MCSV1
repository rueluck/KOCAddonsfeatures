        m += '</table>';
        m += '</td></tr>';

        m += '</div>';

        t.myDiv.innerHTML = m;

        $('#trClearQ').click(function () {
            TRqueueData.list = [];
            TRsaveQueueData();
            t.buildQueueDisplay();
        });

        /* remove sizing restriction therefore allowing 100%

        $("#trQScroll").resizable({
        minWidth: 710,
        maxWidth: 1200,
        minHeight: 200,
        maxHeight: 800,
        stop: function(event, ui) {
        TRupgradeData.upgradeH =  ui.size.height + 'px';
        TRupgradeData.upgradeW =  ui.size.width  + 'px';
        TRsaveUpgradeData();
        }
        });

        $("#trQScroll").css('height', TRupgradeData.upgradeH).css('width', TRupgradeData.upgradeW);

        */

        $('#trEnhanceAddAllQuality').click(function () {
            var low_level = parseInt($('#trEnhanceAllQuality').val());
            var high_level = parseInt($('#trEnhanceAllQualityTo').val());
            if (low_level >= high_level) return;
            for (tr in unsafeWindow.kocThroneItems) {
                var throne_item = unsafeWindow.kocThroneItems[tr];
                if (throne_item.quality == low_level) {
                    var qItem = new QueueItem();
                    qItem.item = tr;
                    qItem.action = "enhance"
                    qItem.level = high_level
                    TRqueueData.list.push(qItem);
                }
            }
            TRsaveQueueData();
            t.buildQueueDisplay();
        });


        $('#trUpgradeAddAllLevel').click(function () {
            var low_level = parseInt($('#trUpgradeAllLevel').val());
            var high_level = parseInt($('#trUpgradeAllLevelTo').val());
            if (low_level >= high_level) return;
            for (tr in unsafeWindow.kocThroneItems) {
                var throne_item = unsafeWindow.kocThroneItems[tr];
                if (throne_item.level == low_level) {
                    var qItem = new QueueItem();
                    qItem.item = tr;
                    qItem.action = "upgrade"
                    qItem.level = high_level
                    TRqueueData.list.push(qItem);
                }
            }
            TRsaveQueueData();
            t.buildQueueDisplay();
        });

