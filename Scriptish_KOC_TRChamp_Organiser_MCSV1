        m += '<div width=100% id=localPresets style="display: ';
        if (GeneralOptions.localPresetHidden) {
            m += 'none;';
        } else {
            m += 'block;';
        }
        m += '"><table width=100%><tr width=100% align=center><td width=15%/><td width=30%/><td/></tr>';



