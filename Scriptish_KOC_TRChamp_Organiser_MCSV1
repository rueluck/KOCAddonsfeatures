            }

            //logit("deleting item: " + id);
            var item = unsafeWindow.kocChampionItems[id];
            if (item) t.setStatus('Salvaging ' + item.name);

            SalvageChampItem(id);
//            var params = unsafeWindow.Object.clone(unsafeWindow.g_ajaxparams);
//            var num_city = pickAetherSalvageCity();
