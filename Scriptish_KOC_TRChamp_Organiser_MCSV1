//                        var sidx = TRsalvageData.upgradedToDelete.indexOf(id);
//                        if (sidx >= 0) {
//                            TRsalvageData.upgradedToDelete.splice(sidx, 1); // Remove item from array
//                            TRsaveSalvageData();
//                        }
//                    }
//                    else {
//                        logit("rslt: " + inspect(rslt, 3, 1));
