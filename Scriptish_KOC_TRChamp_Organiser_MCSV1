            saveGeneralOptions();
        }, false);

        // wait for the current repair to finish before starting
        t.setStatus("Loading ....");

        //$(div).append(m);
        $("#trQDiv").html('<table id="trQueue" class="trTabLined" />');

        $("#trUseORB").change(function () {
